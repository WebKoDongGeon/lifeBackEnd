<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.life.web.repository.MyPageRepository">
    <select id="myPage" parameterType="String" resultType="com.life.web.vo.UserVo">
        SELECT *
        FROM life_user
        WHERE userNo = #{userNo}
    </select>
</mapper>